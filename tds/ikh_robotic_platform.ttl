          hctl:hasOperationType td:readProperty, td:writeProperty
        ];
      td:isObservable false
    ], [ a td:PropertyAffordance;
      td:name "getJointsTarget";
      td:hasForm [
          hctl:hasTarget <http://10.42.0.1:3001/joints_target>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty, td:writeProperty
        ];
      td:isObservable false
    ], [ a td:PropertyAffordance;
      td:name "getJointsVelocity";
      td:hasForm [
          hctl:hasTarget <http://10.42.0.1:3001/joints_velocity>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty, td:writeProperty
        ];
      td:isObservable false
    ], [ a td:PropertyAffordance;
      td:name "getSpray";
      td:hasForm [
          hctl:hasTarget <http://10.42.0.1:3001/spray>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty, td:writeProperty
        ];
      td:isObservable false
    ];
  td:hasActionAffordance [ a td:ActionAffordance;
      td:name "postOperator";
      td:hasUriTemplateSchema [ a js:StringSchema;
          td:name "name"
        ], [ a js:StringSchema;
          td:name "email"
        ];
      td:hasForm [
          <http://www.w3.org/2011/http#methodName> "POST";
          hctl:hasTarget <http://10.42.0.1:3001/Operator>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ]
    ], [ a td:ActionAffordance;
      td:name "deleteOperator";
      td:hasUriTemplateSchema [ a js:StringSchema;
          td:name "token"
        ];
      td:hasForm [
          <http://www.w3.org/2011/http#methodName> "delete";
          hctl:hasTarget <http://10.42.0.1:3001/Operator>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ]
    ], [ a td:ActionAffordance;
      td:name "putTCPTarget";
      td:hasUriTemplateSchema [ a js:StringSchema;
          td:name "pos_y"
        ], [ a js:StringSchema;
          td:name "ori_z"
        ], [ a js:StringSchema;
          td:name "pos_z"
        ], [ a js:StringSchema;
          td:name "ori_y"
        ], [ a js:StringSchema;
          td:name "pos_x"
        ], [ a js:StringSchema;
          td:name "ori_x"
        ], [ a js:StringSchema;
          td:name "ori_w"
        ];
      td:hasForm [
          <http://www.w3.org/2011/http#methodName> "PUT";
          hctl:hasTarget <http://10.42.0.1:3001/tcp/target>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ]
    ], [ a td:ActionAffordance;
      td:name "putTCPVelocity";
      td:hasUriTemplateSchema [ a js:StringSchema;
          td:name "acceleration"
        ], [ a js:StringSchema;
          td:name "velocity"
        ];
      td:hasForm [
          <http://www.w3.org/2011/http#methodName> "PUT";
          hctl:hasTarget <http://10.42.0.1:3001/tcp/velocity>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ]
    ], [ a td:ActionAffordance;
      td:name "initialize";
      td:hasForm [
          <http://www.w3.org/2011/http#methodName> "PUT";
