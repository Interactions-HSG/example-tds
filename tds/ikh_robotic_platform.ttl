@prefix td: <https://www.w3.org/2019/wot/td#> .
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix wotsec: <https://www.w3.org/2019/wot/security#> .
@prefix js: <https://www.w3.org/2019/wot/json-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://localhost:8080/workspaces/uc1/artifacts/ikh_robotic_platform> a td:Thing;
  td:title "IKH Robotic Platform";
  td:hasSecurityConfiguration [ a wotsec:APIKeySecurityScheme;
      wotsec:in "HEADER";
      wotsec:name "X-API-Key"
    ];
  td:hasBase <http://10.42.0.1:3001>;
  td:hasPropertyAffordance [ a td:PropertyAffordance;
      td:name "getOperator";
      td:hasForm [
          hctl:hasTarget <http://10.42.0.1:3001/Operator>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty, td:writeProperty
        ];
      td:isObservable false
    ], [ a td:PropertyAffordance;
      td:name "getTCP";
      td:hasForm [
          hctl:hasTarget <http://10.42.0.1:3001/tcp>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty, td:writeProperty
        ];
      td:isObservable false
    ], [ a td:PropertyAffordance;
      td:name "getTCPTarget";
      td:hasForm [
          hctl:hasTarget <http://10.42.0.1:3001/tcp/target>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty, td:writeProperty
        ];
      td:isObservable false
    ], [ a td:PropertyAffordance;
      td:name "getJoints";
      td:hasForm [
          hctl:hasTarget <http://10.42.0.1:3001/joints>;
