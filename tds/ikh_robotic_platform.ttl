          hctl:hasTarget <http://10.42.0.1:3001/lookback>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ]
    ], [ a td:ActionAffordance;
      td:name "putJointsTarget";
      td:hasForm [
          <http://www.w3.org/2011/http#methodName> "PUT";
          hctl:hasTarget <http://10.42.0.1:3001/joints_target>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ]
    ], [ a td:ActionAffordance;
      td:name "putTeleop";
      td:hasForm [
          <http://www.w3.org/2011/http#methodName> "PUT";
          hctl:hasTarget <http://10.42.0.1:3001/teleop>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ];
      td:hasInputSchema [ a js:ObjectSchema;
          js:properties [ a js:StringSchema;
              js:propertyName "axis_linear"
            ], [ a js:StringSchema;
              js:propertyName "axis_angular"
            ], [ a js:StringSchema;
              js:propertyName "speed_down"
            ], [ a js:StringSchema;
              js:propertyName "deadman"
            ], [ a js:StringSchema;
              js:propertyName "speed_up"
            ];
          js:required "axis_linear", "axis_angular", "speed_up", "speed_down", "deadman"
        ]
    ], [ a td:ActionAffordance;
      td:name "putSpray";
      td:hasForm [
          <http://www.w3.org/2011/http#methodName> "PUT";
          hctl:hasTarget <http://10.42.0.1:3001/spray>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ]
    ] .
