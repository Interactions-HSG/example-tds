@prefix td: <https://www.w3.org/2019/wot/td#> .
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix cherrybot: <http://www.example.org/cherrybot#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix wotsec: <https://www.w3.org/2019/wot/security#> .
@prefix js: <https://www.w3.org/2019/wot/json-schema#> .
@prefix htv: <http://www.w3.org/2011/http#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<urn:cherrybot> a td:Thing, <http://w3id.org/eve#Artifact>;
  dct:title "cherryBot";
  td:hasSecurityConfiguration [ a wotsec:APIKeySecurityScheme;
      wotsec:in "HEADER";
      wotsec:name "Authentication"
    ];
  td:hasBase <https://api.interactions.ics.unisg.ch/cherrybot/>;
  td:hasPropertyAffordance [ a td:PropertyAffordance, cherrybot:GetOperator, js:ObjectSchema,
        cherrybot:Operator_with_Token;
      td:hasForm [
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/cherrybot/operator>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty
        ];
      td:isObservable false;
      js:properties [ a js:StringSchema;
          js:propertyName "name"
        ], [ a js:StringSchema;
          js:propertyName "email"
        ], [ a js:StringSchema;
          js:propertyName "token"
        ];
      js:required "name", "email", "token"
    ], [ a td:PropertyAffordance, cherrybot:getTCP, js:ObjectSchema, cherrybot:Target;
      td:hasForm [
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/cherrybot/tcp>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty
        ];
      td:isObservable false;
      js:properties [ a js:ObjectSchema, cherrybot:Coordinates;
          js:propertyName "coordinate";
          js:properties [ a js:NumberSchema;
              js:propertyName "x";
              js:minimum -7.2E2;
              js:maximum 7.2E2
            ], [ a js:NumberSchema;
              js:propertyName "y";
              js:minimum -7.2E2;
              js:maximum 7.2E2
            ], [ a js:NumberSchema;
              js:propertyName "z";
              js:minimum -1.783E2;
              js:maximum 1.01E3
            ];
          js:required "x", "y", "z"
        ], [ a js:ObjectSchema, cherrybot:Rotation;
          js:propertyName "rotation";
          js:properties [ a js:NumberSchema;
              js:propertyName "roll";
              js:minimum -1.8E2;
              js:maximum 1.8E2
            ], [ a js:NumberSchema;
              js:propertyName "pitch";
              js:minimum -1.8E2;
              js:maximum 1.8E2
            ], [ a js:NumberSchema;
              js:propertyName "yaw";
              js:minimum -1.8E2;
              js:maximum 1.8E2
            ];
          js:required "roll", "pitch", "yaw"
        ];
      js:required "coordinate", "rotation"
    ], [ a td:PropertyAffordance, cherrybot:GetTarget, js:ObjectSchema, cherrybot:Target;
      td:hasForm [
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/cherrybot/tcp/target>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty
        ];
      td:isObservable false;
      js:properties [ a js:ObjectSchema, cherrybot:Coordinates;
          js:propertyName "coordinate";
          js:properties [ a js:NumberSchema;
              js:propertyName "x";
              js:minimum -7.2E2;
              js:maximum 7.2E2
            ], [ a js:NumberSchema;
              js:propertyName "y";
              js:minimum -7.2E2;
              js:maximum 7.2E2
            ], [ a js:NumberSchema;
              js:propertyName "z";
              js:minimum -1.783E2;
              js:maximum 1.01E3
            ];
          js:required "x", "y", "z"
        ], [ a js:ObjectSchema, cherrybot:Rotation;
          js:propertyName "rotation";
          js:properties [ a js:NumberSchema;
              js:propertyName "roll";
              js:minimum -1.8E2;
              js:maximum 1.8E2
            ], [ a js:NumberSchema;
              js:propertyName "pitch";
              js:minimum -1.8E2;
              js:maximum 1.8E2
            ], [ a js:NumberSchema;
              js:propertyName "yaw";
              js:minimum -1.8E2;
              js:maximum 1.8E2
            ];
          js:required "roll", "pitch", "yaw"
        ];
      js:required "coordinate", "rotation"
    ], [ a td:PropertyAffordance, cherrybot:getGripper, js:IntegerSchema;
      td:hasForm [
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/cherrybot/gripper>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty
        ];
      td:isObservable false;
      js:minimum "0"^^xsd:int;
      js:maximum "800"^^xsd:int
    ];
  td:hasActionAffordance [ a td:ActionAffordance, cherrybot:Initialize;
      td:hasForm [
          htv:methodName "PUT";
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/cherrybot/initialize>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ]
    ], [ a td:ActionAffordance, cherrybot:PostOperator;
      td:hasForm [
          htv:methodName "POST";
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/cherrybot/operator>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ];
      td:hasInputSchema [ a js:ObjectSchema, cherrybot:Operator;
          js:properties [ a js:StringSchema;
              js:propertyName "name"
            ], [ a js:StringSchema;
              js:propertyName "email"
            ];
          js:required "name", "email"
        ]
    ], [ a td:ActionAffordance, cherrybot:PutTarget;
      td:hasForm [
          htv:methodName "PUT";
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/cherrybot/tcp/target>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ];
      td:hasInputSchema [ a js:ObjectSchema, cherrybot:TcpMovement;
          js:properties [ a js:IntegerSchema;
              js:propertyName "speed";
              js:minimum "10"^^xsd:int;
              js:maximum "400"^^xsd:int
            ], [ a js:ObjectSchema, cherrybot:Target;
              js:propertyName "target";
              js:properties [ a js:ObjectSchema, cherrybot:Coordinates;
                  js:propertyName "coordinate";
                  js:properties [ a js:NumberSchema;
                      js:propertyName "x";
                      js:minimum -7.2E2;
                      js:maximum 7.2E2
                    ], [ a js:NumberSchema;
                      js:propertyName "y";
                      js:minimum -7.2E2;
                      js:maximum 7.2E2
                    ], [ a js:NumberSchema;
                      js:propertyName "z";
                      js:minimum -1.783E2;
                      js:maximum 1.01E3
                    ];
                  js:required "x", "y", "z"
                ], [ a js:ObjectSchema, cherrybot:Rotation;
                  js:propertyName "rotation";
                  js:properties [ a js:NumberSchema;
                      js:propertyName "roll";
                      js:minimum -1.8E2;
                      js:maximum 1.8E2
                    ], [ a js:NumberSchema;
                      js:propertyName "pitch";
                      js:minimum -1.8E2;
                      js:maximum 1.8E2
                    ], [ a js:NumberSchema;
                      js:propertyName "yaw";
                      js:minimum -1.8E2;
                      js:maximum 1.8E2
                    ];
                  js:required "roll", "pitch", "yaw"
                ];
              js:required "coordinate", "rotation"
            ];
          js:required "target", "speed"
        ]
    ], [ a td:ActionAffordance, cherrybot:PutGripper;
      td:hasForm [
          htv:methodName "PUT";
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/cherrybot/tcp/gripper>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction
        ];
      td:hasInputSchema [ a js:IntegerSchema;
          js:minimum "0"^^xsd:int;
          js:maximum "800"^^xsd:int
        ]
    ] .
