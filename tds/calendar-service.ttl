@prefix td: <https://www.w3.org/2019/wot/td#> .
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix wotsec: <https://www.w3.org/2019/wot/security#> .
@prefix was: <https://was-course.interactions.ics.unisg.ch/wake-up-ontology#> .
@prefix js: <https://www.w3.org/2019/wot/json-schema#> .
@prefix htv: <http://www.w3.org/2011/http#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<urn:calendar-service> a td:Thing, was:CalendarService;
  td:title "A calendar service that can be used for managing a user's events";
  td:hasSecurityConfiguration [ a wotsec:NoSecurityScheme
    ];
  td:hasBase <http://localhost:1080/api/calendar/1/>;
  td:hasPropertyAffordance [ a td:PropertyAffordance, was:ReadUpcomingEvent, js:StringSchema;
      td:name "Read upcoming event";
      td:hasForm [
          htv:methodName "GET";
          hctl:hasTarget <http://localhost:1080/api/calendar/1/upcoming>;
          hctl:forContentType "application/json";
          hctl:hasOperationType td:readProperty
        ];
      td:isObservable false
    ] .
